<div class="container-fluid">
<% if @coworkingspaces == [] || @coworkingspaces.nil? %>
<center><h2>You have no coworking spaces yet</h2></center>
<% else %>
<% @coworkingspaces.each do |space| %>
<% if current_user.id == space.user_id %>
<p>City: <%= space.city %><br>
Address: <%= space.address %><br>
Area in mÂ²: <%= space.area %><br>
Description: <%= space.description %><br>
Photo: <%= image_tag(space.image_url, size: "100x100")%><br>
<%= link_to "Read more", space_path(space.id) %><br></p>
<% end %>
<% end %>
<% end %>
</div>