<% if @suggestionspace.errors.any? %>
   <div id="error_explanation">
     <h2><%= pluralize(@suggestionspace.errors.count, "error") %> </h2>
  
     <ul>
     <% @suggestionspace.errors.full_messages.each do |msg| %>
       <li><%= msg %></li>
     <% end %>
     </ul>
   </div>
 <% end %>
<%= form_for @suggestionspace, url: suggestionspaces_path(coworkingspace_id: @coworkingspace.id) do |f| %>
<p><%= f.date_select :beginDate, start_year: Date.current.year, end_year: Date.current.year + 5 %> <br>
<%= f.date_select :finishDate, start_year: Date.current.year, end_year: Date.current.year + 5 %> <br></p>
<%= f.submit "Ceate date" %>
<% end %>